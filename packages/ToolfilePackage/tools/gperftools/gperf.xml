<tool name="gperf" version="@TOOL_VERSION@">
  <lib name="profiler"/>
  <client>
    <environment name="GPERF_BASE" default="@TOOL_ROOT@"/>
    <environment name="LIBDIR"        default="$GPERF_BASE/lib"/>
  </client>
  <use name="libunwind"/>
  <runtime name="PATH" value="$GPERF_BASE/bin" type="path"/>
</tool>
